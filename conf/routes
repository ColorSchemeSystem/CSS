# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /login/:id                  controllers.Application.logind(id:Long)
GET     /login                      controllers.Application.login()
GET     /logout                     controllers.Application.logout()
GET     /loginEntry                 controllers.Application.loginEntry()
GET     /createAccount              controllers.Application.createAccount()
GET     /saveAccount                controllers.Application.saveAccount()
GET     /myPage/:id                 controllers.Application.myPage(id:Long)

GET     /upload             controllers.AdminController.upload()
POST    /upload             controllers.AdminController.doUpload()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
